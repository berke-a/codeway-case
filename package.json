{
    "name": "codeway-task",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "install:app": "cd app && npm install",
        "install:app-ui": "cd app-ui && npm install",
        "start:app": "cd app && npm run dev",
        "start:app-ui": "cd app-ui && npm run serve",
        "install": "npm run install:app && npm run install:app-ui",
        "start": "concurrently \"npm run start:app\" \"npm run start:app-ui\"",
    }
}